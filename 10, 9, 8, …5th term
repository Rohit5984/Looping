#include <stdio.h>

void printSeries(int n);

int main() {
    int n = 5;   // number of terms we want
    printSeries(n);
    return 0;
}

void printSeries(int n) {
    int term = 10;   // first term
    printf("Series: ");
    for (int i = 1; i <= n; i++) {
        printf("%d ", term);
        term = term-1;      // decrease by 1 each time
    }
    printf("\n5th term = %d\n", term + 1);  // term was decremented one extra time in the loop
}
