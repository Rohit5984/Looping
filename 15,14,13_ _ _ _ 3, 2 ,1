#include <stdio.h>

void printSeries(int n);

int main() {
    int n = 1;   // fixed upper limit
    printSeries(n);
    return 0;
}

void printSeries(int n) {
    printf("Series from 15 to %d: ", n);   // print heading once
    for (int i = 15; i >= n; i--) {
        printf("%d ", i);   // just print numbers with space
    }
    printf("\n");   // final newline after the series
}

