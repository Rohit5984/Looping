#include <stdio.h>

void printSeries(int n);

int main() {
    int n = 15;   // fixed upper limit
    printSeries(n);
    return 0;
}

void printSeries(int n) {
    printf("Series from %d to 1: ", n);  
    for (int i = n; i >= 1; i--) {   // reverse loop
        printf("%d ", i);
    }
    printf("\n");
}
